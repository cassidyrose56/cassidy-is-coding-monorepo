{
  "buildCommand": "cd apps/cassidyiscoding2.0 && pnpm build",
  "outputDirectory": "apps/cassidyiscoding2.0/build",
  "rewrites": [
    { "source": "/toptentastes/:path*", "destination": "https://top-ten-tastes.vercel.app/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
